      <DTS:Variables />
      <DTS:ObjectData>
        <pipeline
          version="1">
          <components>
            <component
              refId="Package\Extract Drug Lists to table\Extract drug list for each patient in cohort"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="Extract drug list for each patient in cohort"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select P.Cohort
	, P.patid
	, Th.eventdate
	, Prod.productname
	, orgc.orgc
	, articgc.articgc
	, imgc.imgc
	, rectgc.rectgc
	, topgc.topgc
	, inhgc.inhgc
	, nasgc.nasgc
	, dmard.dmard
	, biologic.biologic
	, Th.qty
	, Th.ndd
	, Th.numdays
	, Th.numpacks
	, PT.packtype_desc
	, Th.issueseq

from [dbo].[tblPatient] P
	left join (
		select Th.*
		from [dbo].[tblTherapy] Th
			inner join [dbo].[tlkDrugs] Dr
				on Th.prodcode = Dr.prodcode
		) Th
		on P.patid = Th.patid
	left join CPRD.[dbo].[tlkProduct] Prod
		on Th.prodcode = Prod.prodcode
	left join CPRD.[dbo].[tlkPackType] PT
		on Th.packtype = PT.packtype
	
	left join (
		select distinct orgc, prodcode from [GAE].[dbo].[tlkDrugs] where orgc is not null
	) orgc
		on Th.prodcode = orgc.prodcode 

	left join (
		select distinct articgc, prodcode from [GAE].[dbo].[tlkDrugs] where articgc is not null
	) articgc
		on Th.prodcode = articgc.prodcode 

	left join (
		select distinct imgc, prodcode from [GAE].[dbo].[tlkDrugs] where imgc is not null
	) imgc
		on Th.prodcode = imgc.prodcode 

	left join (
		select distinct rectgc, prodcode from [GAE].[dbo].[tlkDrugs] where rectgc is not null
	) rectgc
		on Th.prodcode = rectgc.prodcode 

	left join (
		select distinct topgc, prodcode from [GAE].[dbo].[tlkDrugs] where topgc is not null
	) topgc
		on Th.prodcode = topgc.prodcode 

	left join (
		select distinct inhgc, prodcode from [GAE].[dbo].[tlkDrugs] where inhgc is not null
	) inhgc
		on Th.prodcode = inhgc.prodcode 

	left join (
		select distinct nasgc, prodcode from [GAE].[dbo].[tlkDrugs] where nasgc is not null
	) nasgc
		on Th.prodcode = nasgc.prodcode 

	left join (
		select distinct dmard, prodcode from [GAE].[dbo].[tlkDrugs] where dmard is not null
	) dmard
		on Th.prodcode = dmard.prodcode 

	left join (
		select distinct biologic, prodcode from [GAE].[dbo].[tlkDrugs] where biologic is not null
	) biologic
		on Th.prodcode = biologic.prodcode 

order by eventdate</property>
